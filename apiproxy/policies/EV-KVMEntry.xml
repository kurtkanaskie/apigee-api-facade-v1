    <URIPath>
        <Pattern ignoreCase="false">/v1/organizations/{org_name}/environments/{env_name}/keyvaluemaps/{kvm_name}/entries</Pattern>
        <Pattern ignoreCase="false">/v1/organizations/{org_name}/environments/{env_name}/keyvaluemaps/{kvm_name}/entries/{entry_name}</Pattern>
    </URIPath>
    <JSONPayload>
        <Variable name="entry_name">
